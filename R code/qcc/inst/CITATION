citHeader("To cite qcc in publications use:")

citEntry(
    entry      = "article",
	title        = "qcc: an R package for quality control charting and statistical process control",
	author       = personList(person(given="Luca", family="Scrucca", email="luca.scrucca@unipg.it")),
	journal      = "R News",
	year         = 2004,
	pages        = "11--17",
    volume       = "4/1",
	url          = "https://cran.r-project.org/doc/Rnews/",
    
	textVersion  = 
    paste("Scrucca, L. (2004).",
	      "qcc: an R package for quality control charting and statistical process control.", "R News 4/1, 11-17.")
)

